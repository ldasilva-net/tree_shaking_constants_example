{
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.formatOnSave": true,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "editor.rulers": [
        80
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",    
        "source.organizeImports": "explicit"
    },
}